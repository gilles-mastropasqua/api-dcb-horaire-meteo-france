generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Poste {
  numPoste        String               @id @unique @db.Char(8)
  nomUsuel        String
  commune         String
  lieuDit         String?
  datouvr         DateTime?
  datferm         DateTime?
  posteOuvert     Boolean              @default(false)
  lat             Float
  lon             Float
  lambx           Int?
  lamby           Int?
  alti            Int?
  typePosteActuel Int?
  observations    ObservationHoraire[]

  @@index([posteOuvert])
}

